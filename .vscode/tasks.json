{
    "version": "2.0.0",
    "options": {
        "cwd": "${workspaceRoot}"
    },
    "args": [
        "-c"
    ],
    "tasks": [
        {
            "label": "make",
            "command": "make build",
            "type": "shell",
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher": "$gcc"
        },
        {
            "label": "clean",
            "command": "make clean",
            "type": "shell",
            "group": "build",
            "problemMatcher": "$gcc"
        },
    ]
}